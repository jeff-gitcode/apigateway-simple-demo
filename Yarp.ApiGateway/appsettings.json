{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "CustomerRoute": {
        "ClusterId": "WeatherForcastCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "WeatherForcastCluster": {
        "Destinations": {
          "customerServer": {
            "Address": "https://localhost:7081"
          }
        }
      }
    }
  }
}
